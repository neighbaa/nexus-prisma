{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "tsBuildInfoFile": "node_modules/.cache/.tsbuildinfo-esm",
    "outDir": "dist-esm",
    "module": "ES2015",
    "rootDir": "src",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
