{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "tsBuildInfoFile": "node_modules/.cache/.tsbuildinfo-cjs",
    "outDir": "dist-cjs",
    "module": "commonjs",
    "rootDir": "src",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
